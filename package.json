{
  "name": "devexp",
  "author": "Stanislav Sysoev <d4rkr00t@gmail.com> (http://sysoev.org)",
  "license": "MIT",
  "version": "0.1.0",
  "description": "Developer Experiments",
  "repository": {
    "url": "https://github.com/devexp-org/devexp"
  },
  "main": "start.js",
  "pre-commit": [
    "eslint",
    "test"
  ],
  "precommit.silent": true,
  "scripts": {
    "test": "NODE_ENV=testing mocha --compilers js:babel-register -r ./tests/setup.js -R spec '*/**/__tests__/**/*.js'",
    "test:heavy": "mocha --compilers js:babel-register -r ./tests/setup.js -R spec 'tests/heavy/**.js'",
    "test:watch": "NODE_ENV=testing mocha --compilers js:babel-register -w -r ./tests/setup.js -R spec '*/**/__tests__/**/*.js'",
    "_test": "npm run test --silent || true",
    "_test:heavy": "npm run test:heavy --silent || true",
    "eslint": "eslint .",
    "_eslint": "npm run eslint --silent || true",
    "coverage": "NODE_ENV=testing babel-node ./node_modules/.bin/isparta cover --report text --report html ./node_modules/.bin/_mocha -- -r ./tests/setup.js -R progress '*/**/__tests__/**/*.js'",
    "_coverage": "npm run coverage --silent || true",
    "start": "nodemon start.js -e js,json,html --ignore node_modules/"
  },
  "dependencies": {
    "badgs": "^1.0.0",
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "github": "^0.2.4",
    "lodash": "^3.10.1",
    "minimatch": "^3.0.0",
    "moment": "^2.10.6",
    "mongoose": "^4.2.0",
    "node-xmpp-client": "^3.0.0",
    "proxy-express": "^1.1.2",
    "response-time": "^2.3.1",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "babel": "^6.3.0",
    "babel-cli": "^6.3.0",
    "babel-core": "^6.3.0",
    "babel-eslint": "^4.1.0",
    "babel-plugin-syntax-function-bind": "^6.1.0",
    "babel-plugin-transform-es2015-classes": "^6.3.15",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.3.16",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.0",
    "babel-runtime": "^6.3.0",
    "chai": "^3.4.0",
    "eslint": "^1.10.0",
    "isparta": "^4.0.0",
    "mocha": "^2.3.3",
    "node-architect": "^0.0.7",
    "nodemon": "^1.7.3",
    "pre-commit": "^1.1.2",
    "proxyquire": "^1.7.3",
    "sinon": "^1.17.2",
    "supertest": "^1.1.0"
  }
}
